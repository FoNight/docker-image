FROM busybox:latest
WORKDIR /app
RUN wget  -O sql.zip https://codeload.github.com/opendistro-for-elasticsearch/sql/zip/develop
RUN wget -O amazon-corretto-11-x64-linux-jdk.tar.gz https://corretto.aws/downloads/latest/amazon-corretto-11-x64-linux-jdk.tar.gz

